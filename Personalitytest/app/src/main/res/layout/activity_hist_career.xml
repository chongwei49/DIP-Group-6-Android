<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/profile_resultsbg"
    tools:context=".Tests">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">
        android:layout_alignParentEnd="true"

        <RelativeLayout
            android:id="@+id/userDp"
            android:layout_width="90dp"
            android:layout_height="75dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="30dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="5dp"
                app:srcCompat="@drawable/joblogo" />
        </RelativeLayout>


        <TextView
            android:id="@+id/username"
            android:layout_width="280dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="35dp"
            android:fontFamily="@font/segeodisplaysemibold"
            android:text="Career Test"
            android:textSize="35dp" />

        <androidx.cardview.widget.CardView
            android:id="@+id/personality"
            android:layout_width="400dp"
            android:layout_height="130dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="205dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="232dp"
            app:cardCornerRadius="20dp">

            <RelativeLayout
                android:layout_width="400dp"
                android:layout_height="140dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_gravity="center_horizontal|center_vertical"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/personalityresult"
                android:orientation="vertical">

                <TextView
                    android:layout_width="224dp"
                    android:layout_height="60dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="23dp"
                    android:layout_marginTop="48dp"
                    android:layout_marginEnd="-23dp"
                    android:fontFamily="@font/segeodisplay"
                    android:gravity="left"
                    android:text="You prefer working with groups and mingling with \nyour peers. Working with a team brings out the \nbest in you."
                    android:textColor="#4E4848"
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="224dp"
                    android:layout_height="30dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/segeodisplaysemibold"
                    android:gravity="center"
                    android:text="OUTGOING"
                    android:textColor="#4E4848"
                    android:textSize="23sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/personalityHistory"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginEnd="25dp"
                    android:layout_marginBottom="15dp"
                    android:fontFamily="@font/segeodisplay"
                    android:text="Date: 4/10/22"
                    android:textColor="@color/black"
                    android:textSize="15sp" />


            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <ImageView
            android:id="@+id/homeBtn"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="190dp"
            android:layout_marginTop="625dp"
            app:srcCompat="@drawable/homeicon" />

        <ImageView
            android:id="@+id/backtoresults"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="25dp"
            android:layout_marginTop="625dp"
            app:srcCompat="@drawable/backicon2" />

        <TextView
            android:id="@+id/friendsBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="55dp"
            android:layout_marginTop="625dp"
            android:fontFamily="@font/segeodisplaysemibold"
            android:text="Back"
            android:textSize="20dp" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="625dp"
            android:layout_marginEnd="85dp"
            app:srcCompat="@drawable/profileicon" />

        <TextView
            android:id="@+id/profileBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="625dp"
            android:layout_marginEnd="25dp"
            android:fontFamily="@font/segeodisplaysemibold"
            android:text="Profile"
            android:textSize="20dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="155dp"
            android:layout_marginBottom="160dp"
            android:fontFamily="@font/segeodisplaysemibold"
            android:gravity="center"
            android:text="Your Results"
            android:textColor="#4E4848"
            android:textSize="23sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="360dp"
            android:layout_marginBottom="160dp"
            android:fontFamily="@font/segeodisplaysemibold"
            android:gravity="center"
            android:text="History"
            android:textColor="#4E4848"
            android:textSize="23sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="415dp"
            android:layout_marginBottom="160dp"
            android:fontFamily="@font/segeodisplaysemibold"
            android:gravity="center"
            android:text="OUTGOING"
            android:textColor="#4E4848"
            android:textSize="23sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="290dp"
            android:layout_marginTop="450dp"
            android:layout_marginBottom="160dp"
            android:fontFamily="@font/segeodisplay"
            android:gravity="center"
            android:text="Date: 4/1/21"
            android:textColor="@color/black"
            android:textSize="15sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="290dp"
            android:layout_marginTop="560dp"
            android:layout_marginBottom="160dp"
            android:fontFamily="@font/segeodisplay"
            android:gravity="center"
            android:text="Date: 2/11/20"
            android:textColor="@color/black"
            android:textSize="15sp" />

        <ImageView
            android:layout_width="60dp"
            android:layout_height="78dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="400dp"
            app:srcCompat="@drawable/joblogo" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="60dp"
            android:layout_height="78dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="500dp"
            app:srcCompat="@drawable/joblogo" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="520dp"
            android:layout_marginBottom="160dp"
            android:fontFamily="@font/segeodisplaysemibold"
            android:gravity="center"
            android:text="IDEALISTIC"
            android:textColor="#4E4848"
            android:textSize="23sp"
            android:textStyle="bold" />

    </RelativeLayout>

</RelativeLayout>